<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle" >
            <padding
                android:bottom="3dp"
                android:top="3dp" />
            <solid android:color="#ff9c64b7" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle" >
            <padding
                android:bottom="3dp"
                android:top="3dp" />
            <solid android:color="#ffb358ce" />
        </shape>
    </item>
</selector>